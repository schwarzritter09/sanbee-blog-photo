<%- model_class = Photo -%>
<br />
<br />
<%= form_tag photos_path, :method=>"get", :id=>"search-form" do %>
<div class="panel-group" id="search">
  <div class="panel panel-default">
  	<div class="panel-heading">
  	  <h3 class="panel-title">
  	  	<a data-toggle="collapse", data-parent="#search" href="#members">
  	  	  検索
  	  	</a>
  	  </h3>
  	</div>
  	  <div class="panel-body collapse" id="members">
  	    <%= button_tag "検索する", :type=>"button", :class=>"btn btn-lg btn-default search-button" %>
  	    <%= search_panel %>
  	    <%= button_tag "検索する", :type=>"button", :class=>"btn btn-lg btn-default search-button" %>
  	  </div>
  </div>
</div>
<% end %>
<%= paginate(@photos) %>
<div id="photos" class="container-fluid">
  <div class="row">
    <%= render @photos %>
  </div>
</div>
