<h1>全体情報</h1>
<div class="table-responsive">
  <table class="table table-striped table-bordered">
    <tr>
      <th>全写真枚数</th>
      <th>タグ付け済み枚数</th>
      <th>タグ付け未済み枚数</th>
    </tr>
    <tr>
      <td><%= @allCount %></td>
      <td><%= @tagedCount %></td>
      <td><%= @notTagedCount %></td>
    </tr>
  </table>
</div>
<br>
<h1>掲載回数ランキング</h1>
<div class="table-responsive">
  <table class="table table-striped table-bordered">
    <tr>
      <th>登場回数</th>
      <th>メンバー</th>
    </tr>
    <% @rankOfEntryCount.each do |member_id, count| %>
      <tr>
        <td><%= count %></td>
        <td><%= Member.find(member_id).name %></td>
      </tr>
    <% end %>
  </table>
</div>
