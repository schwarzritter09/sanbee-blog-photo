<div class="table-responsive">
  <table class="table table-striped table-bordered">
    <tr>
      <th>登場回数</th>
      <th>メンバー</th>
    </tr>
    <% @rankOfEntryCount.each do |member_id, count| %>
      <tr>
        <td><%= count %></td>
        <td><%= Member.find(member_id).name %></td>
      </tr>
    <% end %>
  </table>
</div>
